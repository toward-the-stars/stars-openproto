{
  "name": "@toward-the-stars/stars-openproto",
  "version": "1.0.0",
  "description": "stars typescript esnext lib",
  "author": "Kuanwen Lai <kwlai0927@gmail.com> (https://github.com/kwlai0927)",
  "repository": "https://github.com/toward-the-stars/stars-openproto",
  "scripts": {
    "buf:tidy": "npx buf dep update | npx buf dep prune",
    "buf:go": "npx buf generate --template buf.gen.go.yaml",
    "buf:ts": "npx buf generate --template buf.gen.ts.yaml",
    "build": "pnpm buf:tidy | pnpm buf:go | pnpm buf:ts | tsc"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@bufbuild/buf": "^1.47.2",
    "@bufbuild/protobuf": "^2.2.2",
    "@bufbuild/protoc-gen-es": "^2.2.2"
  },
  "packageManager": "pnpm@9.14.4"
}
